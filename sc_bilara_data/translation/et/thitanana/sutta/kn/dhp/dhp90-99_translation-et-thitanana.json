{
  "dhp90:0.1": "LÜHIKESTE KOGU ",
  "dhp90:0.2": "DHAMMAVÄRSID ",
  "dhp90:0.3": "Arahanti grupp ",
  "dhp90:0.4": "",
  "dhp90:1": "Teekonna lõpetanud leinatule, ",
  "dhp90:2": "kõigest täielikult vabanenule, ",
  "dhp90:3": "kõikidest kammitsatest loobunule, ",
  "dhp90:4": "pole kire palavikku enam olemas. ",
  "dhp91:0": "",
  "dhp91:1": "Teadlikud püüdlevad. ",
  "dhp91:2": "Nad ei naudi koduelu. ",
  "dhp91:3": "Nagu luiged jätavad järved, ",
  "dhp91:4": "hülgavad teadlikud kõik peatuspaigad. ",
  "dhp92:1": "Need, kes ei kogu, ",
  "dhp92:2": "on täielikult teadlikud toidu olemusest ",
  "dhp92:3": "ning on saavutanud tühjuse ja märgituse, ",
  "dhp92:4": "nende pärusmaaks on vabanemine. ",
  "dhp92:5": "Nii, nagu lindude lennuteed taevas, ",
  "dhp92:6": "nii on ka nende kulgemist raske mõista. ",
  "dhp93:0": "",
  "dhp93:1": "Kes on tulvad täielikult eemaldanud ",
  "dhp93:2": "ja toitu mittekiindunud, ",
  "dhp93:3": "ning on saavutanud tühjuse ja märgituse, ",
  "dhp93:4": "nende pärusmaaks on vabanemine. ",
  "dhp93:5": "Nii, nagu lindude lennuteed taevas, ",
  "dhp93:6": "on ka nende teed raske mõista. ",
  "dhp94:1": "Kes kulgeb rahunenud võimetega ",
  "dhp94:2": "nagu vankrijuht hästi taltsutatud hobustega, ",
  "dhp94:3": "kes on hüljanud uhkuse ja on tulvadeta —",
  "dhp94:4": "teda, sellist arahanti, kadestavad isegi deevad. ",
  "dhp95:0": "",
  "dhp95:1": "Kes on takistusteta, kindel kui maa, ",
  "dhp95:2": "nagu Indra sammas; ",
  "dhp95:3": "nagu mudast puhastunud tiik —",
  "dhp95:4": "sellise jaoks pole enam sansaarat. ",
  "dhp96:1": "Rahulik on tema meel, ",
  "dhp96:2": "rahulik kõne ja ka käitumine; ",
  "dhp96:3": "õiges tarkuses vabanenud – ",
  "dhp96:4": "selline on täielikult rahunenud. ",
  "dhp97:1": "Usuta ja mittetekkinut teadev, ",
  "dhp97:2": "ning kõik ühendused läbilõiganud mees, ",
  "dhp97:3": "taasilmnematu, ootused hävitanud —",
  "dhp97:4": "tema on tõepoolest ülim inimene. ",
  "dhp98:1": "Külas või metsas, ",
  "dhp98:2": "orus või mäel —",
  "dhp98:3": "kus iganes arahant viibib, ",
  "dhp98:4": "on see koht meeldiv. ",
  "dhp99:1": "Kus on rõõmustavad metsad, ",
  "dhp99:2": "seal ei rõõmusta rahvas; ",
  "dhp99:3": "seal rõõmustavad kiretud, ",
  "dhp99:4": "kes ei otsi naudinguid. ",
  "dhp99:5": "Seitsmenda, arahanti grupi (lõpp). "
}